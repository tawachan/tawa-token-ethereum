{
  "extends": ["standard", "prettier"],
  "env": {
    "browser": true
  },
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "semi": true,
        "trailingComma": "es5"
      }
    ],
    "yoda": 0,
    "no-unused-vars": 1,
    "comma-dangle": ["error", "always-multiline"]
  },
  "globals": {
    "$": false
  }
}
